        <div class="content">
            <div class="container-fluid">
                <div class="row">
                    <div class="col-lg-4 col-md-5">
                        <div class="card card-user">
                            
                          <div class="header">
                                <h4 class="title">Rank</h4>
                            </div>
                            
                            <div class="text-center">
                                <div class="row">
                                    <div class="col-md-3 col-md-offset-1">
                                        <h5>4<br /><small>hakos</small></h5>
                                    </div>
                                    <div class="col-md-4">
                                        <h5>0<br /><small>University Rank</small></h5>
                                    </div>
                                    <div class="col-md-3">
                                        <h5>0<br /><small>public rank</small></h5>
                                    </div>
                                </div>
                            </div>
                        </div>
                        
                            <div class="header">
                                <h4 class="title">Status</h4>
                            </div>
                            

                            
                            <div class="content table-responsive table-full-width">
                                <table class="table table-striped">
                                    <thead>
                                        <th>subject</th>
                                    
                                    	<th>topic</th>
                                    	<th>Date</th>
                                    	<th>Approved</th>
                                    </thead>
                                    <tbody>
                                        <tr>
                                        	<td>java</td>
                                        	<td>inheritence</td>
                                        	<td>12/2/2016</td>
                                                <td>yes</td>
                                        	</tr>
                                    </tbody>
                                </table>

                            </div>
                        </div>
                

                 
                    <div class="col-lg-8 col-md-7">
                        <div class="card">
                            <div class="header">
                                <h4 class="title">Upload file</h4>
                            </div>
                            <div class="content">
                                <form>
                                    <div class="row">
                                        <div class="col-md-5">
                                            <div class="form-group">
                                                <label>University Name</label>
                                                <input type="text" class="form-control border-input" disabled placeholder="Chitkara University" value="Chitkara University">
                                            </div>
                                        </div>
                                        <div class="col-md-3">
                                            <div class="form-group">
                                                <label>Username</label>
                                                <input type="text" class="form-control border-input"  disabled placeholder="Username" value="username">
                                            </div>
                                        </div>
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                <label for="exampleInputEmail1">Email address</label>
                                                <input type="email" class="form-control border-input"  disabled placeholder="Email" value="Email">
                                            </div>
                                        </div>
                                    </div>

                                    
                                    
                                        <div class="col-md-4">
                                            <div class="form-group">
                                                
                                                <input type="file" class="form-control border-input" multiple>
                                            </div>
                                        </div>
                                    </div>

                                   
                                    <div class="text-center">
                                        <button type="submit" class="btn btn-info btn-fill btn-wd">Upload</button>
                                    </div>
                                    <div class="clearfix"></div>
                                </form>
                            </div>
                        </div>
                    </div>


                </div>
            </div>
